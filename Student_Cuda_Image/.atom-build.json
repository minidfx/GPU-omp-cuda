{
  "cmd": "bash",
  "name": "Compile",
  "args": [ "build.sh" ],
  "sh": false,
  "env": {},
  "errorMatch": [
    "\n(?<file>[\\/0-9a-zA-Z\\._ ]+\\.[\\w\\d]+):(?<line>\\d+):(?<col>\\d+)",
    "(?<file>[^\\r\\n(]+)\\((?<line>\\d+)\\)"
  ],
  "targets": {
    "Compile & Run (command line)": {
      "cmd": "bash",
      "args": [ "buildAndRun.sh" ]
    },
    "Run (command line)": {
      "cmd": "bash",
      "args": [ "run.sh" ]
    },
    "3 runs (command line)": {
      "cmd": "bash",
      "args": [ "3runs.sh" ]
    },
    "Clean": {
      "cmd": "bash",
      "args": [ "clean.sh" ]
    }
  }
}
